{
    "name": "Spp/ProductBuilder",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "timothyasp/nova-color-field": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spp\\ProductBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spp\\ProductBuilder\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
